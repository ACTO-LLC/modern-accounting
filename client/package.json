{
    "name": "client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "playwright test",
        "test:ui": "playwright test --ui",
        "test:bank": "playwright test bank-transactions.spec.ts",
        "test:headed": "playwright test --headed",
        "test:unit": "vitest run",
        "test:unit:watch": "vitest",
        "demo": "playwright test --config=playwright.demo.config.ts --project=demo-landscape",
        "demo:vertical": "playwright test --config=playwright.demo.config.ts --project=demo-vertical",
        "demo:square": "playwright test --config=playwright.demo.config.ts --project=demo-square",
        "demo:headed": "playwright test --config=playwright.demo.config.ts --project=demo-landscape --headed",
        "demo:report": "playwright show-report demo-report"
    },
    "dependencies": {
        "@azure/msal-browser": "^3.0.0",
        "@azure/msal-react": "^2.0.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@hookform/resolvers": "^5.2.2",
        "@mui/material": "^7.3.7",
        "@mui/x-data-grid": "^8.24.0",
        "@radix-ui/react-dialog": "^1.0.0",
        "@radix-ui/react-dropdown-menu": "^2.0.0",
        "@radix-ui/react-slot": "^1.0.0",
        "@tanstack/react-query": "^5.0.0",
        "@tanstack/react-table": "^8.0.0",
        "axios": "^1.6.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "dompurify": "^3.3.1",
        "fuse.js": "^7.1.0",
        "lucide-react": "^0.290.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.0.0",
        "react-plaid-link": "^3.6.1",
        "react-router-dom": "^7.9.6",
        "recharts": "^2.0.0",
        "sonner": "^2.0.7",
        "tailwind-merge": "^2.0.0",
        "zod": "^3.0.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@types/dompurify": "^3.0.5",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "@vitest/ui": "^4.0.17",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vitest": "^4.0.17"
    }
}
